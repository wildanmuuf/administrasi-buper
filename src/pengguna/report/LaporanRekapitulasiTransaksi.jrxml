	<subDataset name="Transaksi" uuid="780af9d6-9427-4f81-a778-383f12bb00b2">
		<parameter name="Tahun" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT transaksi.tanggal_transaksi, monthname(transaksi.tanggal_transaksi) as bulan, ifnull((Select sum(trx.total_tagihan) from transaksi trx WHERE year(trx.tanggal_transaksi) = year(transaksi.tanggal_transaksi) and month(trx.tanggal_transaksi) = month(transaksi.tanggal_transaksi)),0) as total FROM `transaksi` where year(transaksi.tanggal_transaksi) = $P{Tahun} GROUP BY bulan ORDER BY month(transaksi.tanggal_transaksi)]]>
		</queryString>
		<field name="tanggal_transaksi" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="bulan" class="java.lang.String"/>
		<field name="total" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="Tahun" class="java.lang.String">
		<defaultValueExpression><![CDATA[$F{tahun}]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     MONTHNAME(transaksi.`tanggal_transaksi`) AS nama_bulan,
year(transaksi.tanggal_transaksi) as tahun,
month(transaksi.tanggal_transaksi) as bulan
FROM
     `transaksi` transaksi]]>
	</queryString>
